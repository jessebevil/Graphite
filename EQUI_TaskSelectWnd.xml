<Button item="TSEL_DeclineButton">
	<ScreenID>DeclineButton</ScreenID>
	<Size>
		<CX>100</CX>
		<CY>20</CY>
	</Size>
	<DrawTemplate>WDT_HotButton</DrawTemplate>
	<Font>2</Font>
	<MouseoverColor>
		<R>255</R>
		<G>255</G>
		<B>255</B>
	</MouseoverColor>
	<PressedColor>
		<R>255</R>
		<G>0</G>
		<B>0</B>
	</PressedColor>
	<Style_Border>true</Style_Border>
	<Template>20_HotButton</Template>
	<Text>Decline All</Text>
	<TextColor>
		<R>255</R>
		<G>0</G>
		<B>0</B>
	</TextColor>
	<UseCustomMouseoverColor>true</UseCustomMouseoverColor>
	<UseCustomPressedColor>true</UseCustomPressedColor>
</Button>

