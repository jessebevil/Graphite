	<LeftAnchorOffset>0</LeftAnchorOffset>
	<LeftAnchorToLeft>true</LeftAnchorToLeft>
	<RightAnchorOffset>305</RightAnchorOffset>
	<RightAnchorToLeft>true</RightAnchorToLeft>
	<BottomAnchorOffset>26</BottomAnchorOffset>
	<BottomAnchorToTop>false</BottomAnchorToTop>
	<DrawTemplate>WDT_Glass_16x16</DrawTemplate>
	<Style_Border>true</Style_Border>
		<Pieces>Glass_BG</Pieces>
		<Pieces>TSEL_TaskList</Pieces>
</Screen>

